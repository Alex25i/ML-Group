    "    for attr_indices_index in range(len(ATTRIBUTE_INDICES)):\n",
    "        first_index = ATTRIBUTE_INDICES[attr_indices_index][0]\n",
    "        last_index = ATTRIBUTE_INDICES[attr_indices_index][-1]\n",
    "        attribute = X_test.T[first_index:last_index + 1]\n",
